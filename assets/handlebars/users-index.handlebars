{{#each awards as |award|}}
  {{log "hi"}}
  <section data-id="{{award.id}}">
    <h6>Type:</h6>
    <h3>
      {{award.level}}
    </h3>
    <h6>Title of</h6><h6>{{award.name}}</h6>
    <p>
      {{#if award.user_id}}
        {{award.user.id}}
      {{else}}
        No users have this award
      {{/if}}
    </p>
  </section>
{{/each}}